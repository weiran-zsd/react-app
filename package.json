{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "lint": "eslint --ext=.js,.jsx,.ts,.tsx .",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "ignorePatterns": [
      "/dist/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/vite-react": "^1.0.1",
    "@types/react": "^17.0.60",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "latest",
    "c8": "^7.14.0",
    "eslint-config-react-app": "^7.0.1",
    "jsdom": "^20.0.3",
    "typescript": "latest",
    "vite": "latest",
    "vitest": "^0.32.0"
  }
}
